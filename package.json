{
  "name": "minigame-coresdk",
  "version": "0.8.6",
  "private": false,
  "description": "minigame sdk core libs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint src test",
    "dev": "rimraf dist types && cross-env NODE_ENV=development webpack-dev-server --config ./scripts/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./scripts/webpack.prod.js && copyfiles -f types/index.d.ts dist/",
    "publish": "npm publish",
    "release": "standard-version",
    "release-major": "standard-version --release-as major",
    "release-minor": "standard-version --release-as minor",
    "release-patch": "standard-version --release-as patch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lolizeppelin/minigame-coresdk"
  },
  "author": "lolizeppelin",
  "license": "MIT",
  "homepage": "https://github.com/lolizeppelin/minigame-coresdk",
  "dependencies": {
    "js-sha1": "0.7.0",
    "ts-md5": "^1.3.1",
    "url-search-params-polyfill": "^8.2.5",
    "loglevel": "^1.9.1"
  },
  "devDependencies": {
    "minigame-typings": "^0.4.3",
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/runtime-corejs3": "^7.23.2",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/jest": "^27.0.3",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "dts-bundle-generator": "^8.1.2",
    "dts-bundle-webpack": "^1.0.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.10.0",
    "webpackbar": "^5.0.2",
    "clean-webpack-plugin": "^4.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint"
    ]
  }
}
